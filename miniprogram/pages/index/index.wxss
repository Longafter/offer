/* 轮播图 */
swiper {
    width: 100%;
    height: 420rpx;
}
swiper image {
    width: 100%;
    height: 100%;
}
/* 猪分类列表 */
.cate-list {
    display:flex;
    flex-wrap: wrap;
}
.box {
    width:50%;
    height: 330rpx;
}
.cate-item {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.box image {
    width: 100rpx;
    height: 80rpx;
}